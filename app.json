{
  "expo": {
    "name": "eGovSA",
    "slug": "eGovSA",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash.jpg",
    "scheme": "egovsa",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.itmda.eGovSA",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "icon": "./assets/images/splash.jpg",
      "adaptiveIcon": {
        "backgroundColor": "#de6c0fff",
        "foregroundImage": "./assets/images/splash.jpg",
        "monochromeImage": "./assets/images/splash.jpg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.itmda.eGovSA"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      "expo-sqlite",
      [
        "expo-notifications",
        {
          "color": "#E67E22",
          "mode": "production"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4afc4c09-33f6-48d6-b57a-58b2edfb18bb"
      }
    },
    "owner": "itmda"
  }
}
